{
  "compilerOptions": {
    "module": "ES2015",
    "outDir": "./dist",
    "allowJs": true,
    "target": "es2015",
    "sourceMap": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "bundler",
  },
  "include": [
    "./src/**/*",
    "./test/**/*",
    "Assert.ts"
  ]
}
